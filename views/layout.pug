html
    head
        title MMS
        block topscripts
            meta(charset='utf-8')
            meta(http-equiv='X-UA-Compatible' content='IE=edge')
            meta(name='viewport' content='width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no')
            link(rel='icon' type='image/x-icon' href='/images/icons/LMLogo.ico')
            link(href='/assets/css/loader.css' rel='stylesheet' type='text/css')
            script(src='/assets/js/loader.js' type="text/javascript")
            // BEGIN GLOBAL MANDATORY STYLES
            link(href='https://fonts.googleapis.com/css?family=Nunito:400,600,700' rel='stylesheet')
            link(href='/bootstrap/css/bootstrap.min.css' rel='stylesheet' type='text/css')
            link(href='/assets/css/plugins.css' rel='stylesheet' type='text/css')
            // END GLOBAL MANDATORY STYLES
            // BEGIN LOADER
    body
        #load_screen
            .loader
                .loader-content
                    .spinner-grow.align-self-center
    block headers
        .header-container.fixed-top
            header.header.navbar.navbar-expand-sm
                a.sidebarCollapse(href='javascript:void(0);' data-placement='bottom')
                    svg.feather.feather-menu(xmlns='http://www.w3.org/2000/svg' width='24' height='24' viewbox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round')
                        line(x1='3' y1='12' x2='21' y2='12')
                        line(x1='3' y1='6' x2='21' y2='6')
                        line(x1='3' y1='18' x2='21' y2='18')
                ul.navbar-item.flex-row
                    li.nav-item.align-self-center.page-heading
                        h4(style="color: #b38600;vertical-align: middle;") Milk Management System
                ul.navbar-item.flex-row.search-ul
                    li.nav-item.align-self-center.search-animated
                ul.navbar-item.flex-row.navbar-dropdown
                    li.nav-item
                        a.nav-link(href='/logout')
                            svg.feather.feather-log-out(xmlns='http://www.w3.org/2000/svg' width='24' height='24' viewbox='0 0 24 24' fill='none' stroke='red' stroke-width='2' stroke-linecap='round' stroke-linejoin='round')
                                path(d='M9 21H5a2 2 0 0 1-2-2V5a2 2 0 0 1 2-2h4')
                                polyline(points='16 17 21 12 16 7')
                                line(x1='21' y1='12' x2='9' y2='12')

    block maincontent
        #container.main-container
            .overlay
            .search-overlay
            .sidebar-wrapper.sidebar-theme
                block sidebar
                    nav#sidebar
                        ul.navbar-nav.theme-brand.flex-row.text-center
                            li.nav-item.theme-logo
                                a(href='/dashboard')
                                    img.navbar-logo(src='/images/icons/LMLogo.ico' alt='logo')
                            li.nav-item.theme-text
                                a.nav-link(href='index-2.html')  Admin 
                            li.nav-item.toggle-sidebar
                                svg.feather.feather-arrow-left.sidebarCollapse(xmlns='http://www.w3.org/2000/svg' width='24' height='24' viewbox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round')
                                    line(x1='19' y1='12' x2='5' y2='12')
                                    polyline(points='12 19 5 12 12 5')
                        .shadow-bottom
                        ul#accordionExample.list-unstyled.menu-categories
                            li.menu.active
                                a.dropdown-toggle(href='/' aria-expanded='true')
                                    div
                                        svg.feather.feather-activity(xmlns='http://www.w3.org/2000/svg' width='24' height='24' viewbox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round')
                                            polyline(points='22 12 18 12 15 21 9 3 6 12 2 12')
                                        span Dashboard
                            li.menu
                                a.dropdown-toggle(href='#users' data-toggle='collapse' aria-expanded='false')
                                    div
                                        svg.feather.feather-users(xmlns='http://www.w3.org/2000/svg' width='24' height='24' viewbox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round')
                                            path(d='M17 21v-2a4 4 0 0 0-4-4H5a4 4 0 0 0-4 4v2')
                                            circle(cx='9' cy='7' r='4')
                                            path(d='M23 21v-2a4 4 0 0 0-3-3.87')
                                            path(d='M16 3.13a4 4 0 0 1 0 7.75')
                                        span Manage User
                                    div
                                        svg.feather.feather-chevron-right(xmlns='http://www.w3.org/2000/svg' width='24' height='24' viewbox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round')
                                            polyline(points='9 18 15 12 9 6')
                                ul#users.collapse.submenu.list-unstyled(data-parent='#accordionExample')
                                    li
                                        a(href='/dairy/list')  List of User


            block content
                #content.main-content
                    .layout-px-spacing
                        .row.layout-top-spacing
                            p This is a layout Pages
                            p #{obj._token}
                            p #{obj.message}

            block footer
                .footer-wrapper
                    .footer-section.f-section-1
                    .footer-section.f-section-2
                        p
                        | Coded with 
                        svg.feather.feather-heart(xmlns='http://www.w3.org/2000/svg' width='24' height='24' viewbox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round')
                            path(d='M20.84 4.61a5.5 5.5 0 0 0-7.78 0L12 5.67l-1.06-1.06a5.5 5.5 0 0 0-7.78 7.78l1.06 1.06L12 21.23l7.78-7.78 1.06-1.06a5.5 5.5 0 0 0 0-7.78z')
                        span(style="color:black")  
                            //- a(href="https://www.linkedin.com/in/sanket-vanani-6824a8180/" rel="email") By Sanket Vanani
                            a(href="mailto:sankethv8509@gmail.com" rel="email") By Sanket Vanani
    block bottomscripts                                
        script(src='/assets/js/libs/jquery-3.1.1.min.js').
        script(src='/bootstrap/js/popper.min.js').
        script(src='/bootstrap/js/bootstrap.min.js').
        script(src='/plugins/perfect-scrollbar/perfect-scrollbar.min.js').
        script(src='/assets/js/app.js').
        script(type="text/javascript").
            $(document).ready(function() {
                App.init();
            });
        script(src='/assets/js/custom.js').
        // END GLOBAL MANDATORY SCRIPTS
